.unity-progress-bar__background {
    border-radius: 10px 10px 10px 10px;
    border-width: 2px;
    border-style: solid;
    border-color: rgb(6, 58, 9);
    background-color: rgb(0, 0, 15);
}

.unity-progress-bar__progress {
    border-radius: 5px 0 0 5px;
    background-color: rgb(0, 255, 13);
}

